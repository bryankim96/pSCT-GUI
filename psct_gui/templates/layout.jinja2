<!DOCTYPE html>
<html lang="{{request.locale_name}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="pSCT Alignment GUI">
    <meta name="author" content="Bryan Kim">
    <link rel="icon" href="{{request.static_url('psct_gui:static/cta_logo.png')}}">
    <link href="{{request.static_url('psct_gui:dist/vendors.css')}}" rel="stylesheet">
    <title>pSCT Alignment GUI - {% block title %}{% endblock %}</title>

    {% block head %}
    {% endblock %}
  </head>
  <body>
    {% block content %}
    {% endblock %}
    <script src="{{request.static_url('psct_gui:dist/runtime.bundle.js')}}" type="text/javascript"></script>
    <script src="{{request.static_url('psct_gui:dist/vendors.bundle.js')}}" type="text/javascript"></script>
    {% block js %}
    {% endblock %}
  </body>
</html>
